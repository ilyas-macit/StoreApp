@model IEnumerable<Order> 

<h1 class="text-center">Order</h1>
@if (Model.Count() > 0)
{
    foreach (var order in Model)
    {
        <div>
            <partial name="_OrderDetails" model="order"/>
        </div>
    }
}
else
{
    <div class="alert alert-danger">
        <p class="lead">There is no order yet.</p>
    </div>
}